{% extends "layout.njk" %}

 {% block head%}
    <title>Sobre - Daniel Azevêdo</title>
{% endblock %}

{% block content %}

    <div id="wrapper">
        <img src="{{about.avatar_url}}" alt="Daniel Azevêdo">
        <h1>{{about.name}}</h1>
        <h2>{{about.role}}</h2>
        <p>{{about.description}} </p>
    </div>

    
    <div id="footer-links" class="links" >
        {% for link in about.links%}
        <a href="{{link.url}}" target="_blank">{{link.name}}</a>
        {% endfor %}
    </div>
    
    
{% endblock %}
